    "def twoSum(nums, target):\n",
    "        for index,num in enumerate(nums[:-1]):\n",
    "            x = nums[index] + nums[index+1]\n",
    "            if (x==target):\n",
    "                print(x)\n",
    "                return [index,index+1]"
